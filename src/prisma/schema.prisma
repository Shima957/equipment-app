generator client {
  provider = "prisma-client-js"
}

datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

model User {
  id        String  @id @db.Uuid
  username  String
  avatorUrl String?
  email     String  @unique
}

model DAW {
  id       Int     @id @default(autoincrement())
  category String
  name     String
  maker    String
  webUrl   String
  imgUrl   String?
}
